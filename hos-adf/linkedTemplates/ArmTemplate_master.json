{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "hos-adf"
		},
		"adls_ls_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'adls_ls'"
		},
		"databricks_ls_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'databricks_ls'"
		},
		"hosa_sql_ls_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'hosa_sql_ls'"
		},
		"adls_ls_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://hcadls.dfs.core.windows.net/"
		},
		"hosa_sql_ls_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "thospital.database.windows.net"
		},
		"hosa_sql_ls_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "@{linkedService().db_name}"
		},
		"hosa_sql_ls_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "raja"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "hos-adf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"adls_ls_accountKey": {
						"value": "[parameters('adls_ls_accountKey')]"
					},
					"databricks_ls_accessToken": {
						"value": "[parameters('databricks_ls_accessToken')]"
					},
					"hosa_sql_ls_password": {
						"value": "[parameters('hosa_sql_ls_password')]"
					},
					"adls_ls_properties_typeProperties_url": {
						"value": "[parameters('adls_ls_properties_typeProperties_url')]"
					},
					"hosa_sql_ls_properties_typeProperties_server": {
						"value": "[parameters('hosa_sql_ls_properties_typeProperties_server')]"
					},
					"hosa_sql_ls_properties_typeProperties_database": {
						"value": "[parameters('hosa_sql_ls_properties_typeProperties_database')]"
					},
					"hosa_sql_ls_properties_typeProperties_userName": {
						"value": "[parameters('hosa_sql_ls_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}